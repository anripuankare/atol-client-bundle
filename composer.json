{
    "name": "anripuankare/atol-client-bundle",
    "description": "Symfony Bundle for integration with anripuankare/atol-client",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        },
        {
            "name": "Sokolov Artem",
            "email": "a.sokolov@reg.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.1",
        "anripuankare/atol-client": "^1.6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "jms/serializer-bundle": "^3.3 || ^3.4",
        "guzzlehttp/guzzle": "^6.0 || ^7.3.0"
    },
    "require-dev": {
        "symfony/symfony": "^5.4 || ^6.0",
        "phpunit/phpunit": "^5.7.10 || ^8.5",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\AtolClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\AtolClientBundle\\Tests\\": "tests/"
        }
    }
}
